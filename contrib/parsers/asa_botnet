<!-- 10/11/2016 --- Brian Kellogg -->
<ruleset name="CISCO_BOTNET" id='12002'>
   <pattern>ossec_archive</pattern>
        <rules>
                <rule provider="ELSA" class='12002' id='12002'>
                <patterns>
                <pattern>@ESTRING::ASA-4-338002@@ESTRING::monitored blacklisted @@ESTRING:i0: @@ESTRING::INSIDE:@@ESTRING:i1:/@@ESTRING:i2: (@@ESTRING::OUTSIDE:@@ESTRING:i3:/@@ESTRING:i4: (@@ESTRING::dynamic list: @@ESTRING:s0:,@@ESTRING::threat-level: @@ESTRING:s1:,@@ESTRING::category: @@ANYSTRING:s2:@</pattern>
                </patterns>
                <examples>
                        <example>
                                <test_message program="ossec_archive">Oct 08 2016 22:37:54: %ASA-4-338002: Dynamic Filter monitored blacklisted TCP traffic from INSIDE:10.1.1.1/59293 (10.1.1.1/59293) to OUTSIDE:1.1.1.1/80 (1.1.1.1/80), destination 1.1.1.1 resolved from dynamic list: test.test.com, threat-level: very-high, category: Malware</test_message>
                                <!-- Protocol -->
                                <test_value name="i0">TCP</test_value>
                                <!-- SRC IP -->
                                <test_value name="i1">10.1.1.1</test_value>
                                <!-- SRC Port -->
                                <test_value name="i2">59293</test_value>
                                <!-- DST IP -->
                                <test_value name="i3">1.1.1.1</test_value>
                                <!-- DST Port -->
                                <test_value name="i4">80</test_value>
                                <!-- Domain name -->
                                <test_value name="s0">test.test.com</test_value>
                                <!-- Threat level -->
                                <test_value name="s1">very-high</test_value>
                                <!-- Category -->
                                <test_value name="s2">Malware</test_value>
                        </example>
                    </examples>
                </rule>
        </rules>
</ruleset>
